#!/bin/sh

COMMUNITYSTRING=communitystring
SNMPVERSION=2c
SNMPSERVER=w.x.y.z
OID=1.3.6.1.4.1.12356.101.12.2.4.1.3
SNMPWALKPATH=/usr/bin/snmpwalk

# show logged in users
$SNMPWALKPATH -v$SNMPVERSION -c$COMMUNITYSTRING $SNMPSERVER $OID

# number of logged in users
# snmpwalk -v$SNMPVERSION -c$COMMUNITYSTRING $SNMPSERVER iso.3.6.1.4.1.12356.101.12.2.3.1
